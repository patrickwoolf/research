@article{ ISI:000374958600002,
Author = {Hatipoglu, Esra and Ozgur, Nurgul and Niyazoglu, Mutlu and Dikici,
   Atilla Suleyman and Ekmekci, Ozlem Balci and Yalin, Serkan and Siva,
