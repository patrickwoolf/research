EISSN = {{1619-7089}},
Keywords = {{Atherosclerosis; Chronic kidney disease; FDGPET/CT; Inflammation}},
Keywords-Plus = {{POSITRON-EMISSION-TOMOGRAPHY; ATHEROSCLEROTIC PLAQUE INFLAMMATION;
